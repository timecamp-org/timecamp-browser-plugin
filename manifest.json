    "homepage_url": "http://www.timecamp.com/",
    "incognito": "spanning",
    "offline_enabled": false,
    "permissions": [
        "tabs",
        "storage",
        "https://*.timecamp.com/*"
    ],
    "background": {
        "scripts": ["scripts/lib/jquery-2.1.1.min.js", "scripts/lib/jquery-ui.min.js", "scripts/lib/moment.min.js", "scripts/lib/moment-range.min.js",
          "scripts/config.js", "scripts/TokenManager.js", "scripts/ApiService.js", "scripts/common.js", "scripts/TimerBase.js","scripts/TimerButton.js",
          "scripts/background.js"],
        "persistent": false
    },
    "web_accessible_resources": [
        "images/*.gif",
        "images/*.png",
        "scripts/interceptHistoryChanges.js",
        "templates/*",
        "fonts/*"
    ]
}
